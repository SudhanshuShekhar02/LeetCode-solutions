class Solution {
public:
    vector<bool> prefixesDivBy5(vector<int>& nums) {
        vector<bool>ans;
        int val=0;
        int n=nums.size();
        for(int i=0;i<n;i++){
            val = (val * 2 + nums[i]) % 5;

            if(val==0)
            ans.push_back(true);

            else
            ans.push_back(false);
        }
        return ans;
        
    }
};
