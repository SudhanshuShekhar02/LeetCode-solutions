class Solution {
public:
    int findNumbers(vector<int>& nums) {
        int cnt=0;
        for(int i=0;i<nums.size();i++){
            if(iseven(nums[i])){
                cnt++;
            }
        }
        return cnt;
        
    }
    bool iseven(int num){
        int cnt=0;
        while(num>0){
            num=num/10;
            cnt++;
        }
        return cnt%2==0;
    }
};
