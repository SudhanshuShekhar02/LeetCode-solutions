class Solution {
public:
    vector<int> singleNumber(vector<int>& nums) {
        vector<int>ans;
        int n=nums.size();
        sort(nums.begin(),nums.end());
        for(int i=0;i<n;i++){
            if ((i == 0 || nums[i] != nums[i - 1]) && 
                (i == n - 1 || nums[i] != nums[i + 1])){
                ans.push_back(nums[i]);
            }
        }
        return ans;
        
    }
};
