class Solution {
public:
    int beautifulSubstrings(string s, int k) {
        int butful=0;
        for(int i=0;s[i];i++){
            int vol=0;
            int con=0;

            for(int j=i;s[j];j++){
                if(s[j]=='a'||s[j]=='e'||s[j]=='i'||s[j]=='o'||s[j]=='u'){
                    vol++;
                }else{
                    con++;
                }
                if(vol==con && (vol*con)%k==0){
                    butful++;
                }
            }
        }
        return butful;
        
    }
};
