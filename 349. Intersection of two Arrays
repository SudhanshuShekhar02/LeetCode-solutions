class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        map<int, int>a;
        vector<int> ans;
        for(int i=0;i<nums1.size();i++){
            a[nums1[i]]=1;
        }
            for(int j=0;j<nums2.size();j++){
                if(a[nums2[j]]==1){
                    ans.push_back(nums2[j]);
                    a[nums2[j]]++;
                
            }
        }
        return ans;
        
    }
};
