class Solution {
public:
    bool canConstruct(string ransomNote, string magazine) {
        unordered_map<char, int> unknown;
        for(auto& ch : magazine){
            unknown[ch]++;
        }
        for(auto& ch : ransomNote){
            unknown[ch]--;
            if(unknown[ch]<0){
                return false;
            }
        }
        return true;
    }
};
