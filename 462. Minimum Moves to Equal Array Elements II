class Solution {
public:
    int minMoves2(vector<int>& nums) {
        int n=nums.size();
        sort(nums.begin(),nums.end());
        int ans=0, j=0;
        for(int i=n-1;i>0;i--){
            if(j<i){
                ans+=nums[i]-nums[j];
                j++;
            }
        }
        return ans;
    }
};
