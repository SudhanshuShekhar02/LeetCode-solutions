class Solution {
public:
    int findPoisonedDuration(vector<int>& timeSeries, int duration) {
        int n=timeSeries.size();
        int store=0;
        for(int i=0;i<n-1;i++){
            int temp=timeSeries[i+1]-timeSeries[i];
            store+=min(temp, duration);
        }
        return store+duration;
        
    }
};
