class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int maxprofits= 0;
        int bestBuy=prices[0];
        for(int i=1;i<n;i++){
            if(prices[i]>bestBuy){
                maxprofits=max(maxprofits, prices[i]-bestBuy);
            }
        bestBuy=min(bestBuy, prices[i]);
        }
        return maxprofits;
        
    }
};
